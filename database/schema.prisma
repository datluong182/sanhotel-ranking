datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model tbObjectTrips {
  id                 Int      @id @default(autoincrement())
  name               String
  url                String
  rank               Int
  score              Float
  numberScoreReviews Int[]
  updatedAt          DateTime
}

model tbObjectBookings {
  id                 Int      @id @default(autoincrement())
  name               String
  url                String
  score              Float
  numberScoreReviews Int[]
  updatedAt          DateTime
}
